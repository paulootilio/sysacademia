
var table = document.getElementById("myTable").getElementsByTagName('tbody')[0];

var input = document.createElement("input");
input.setAttribute("type", "hidden");
input.setAttribute("id", "<%= @exercicio[0].id %>" )
input.setAttribute("name", "<%= @nameExercicio %>");
input.setAttribute("value", "<%= @exercicio[0].id %>");
document.getElementById("formtreino").appendChild(input);

document.getElementById("exerciciotamanho").value = "<%=  @tamanho %>";
// Create an empty <tr> element and add it to the 1st position of the table:
var row = table.insertRow(table.rows.length);




// Insert new cells (<td> elements) at the 1st and 2nd position of the "new" <tr> element:
var cell1 = row.insertCell(0);
var cell2 = row.insertCell(1);
var cell3 = row.insertCell(2);
var cell4 = row.insertCell(3);
var cell5 = row.insertCell(4);
var cell6 = row.insertCell(5);


//ocultar primeira coluna
$('td:nth-child(1)').hide();


// Add some text to the new cells:
cell1.innerHTML = "<%= @exercicio[0].id %>";
cell2.innerHTML = "<%= @exercicio[0].nome %>";
cell3.innerHTML = "<%= @exercicio[0].serie %>";
cell4.innerHTML = "<%= @exercicio[0].rept %>";
cell5.innerHTML = "<%= @exercicio[0].carga %>";
cell6.innerHTML = "<button onclick='Remover(this, 0)' class='btn btn-primary'>Remover</button> ";











